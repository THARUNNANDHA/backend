{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "main": "index.js",
            "scripts": "node index.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://opentuf-jwt-node.vercel.app"
            }
        }
    ]
}